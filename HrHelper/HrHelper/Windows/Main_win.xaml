<Window x:Class="HrHelper.Windows.Main_win"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HrHelper.Windows"
        mc:Ignorable="d"
        Title="Main_win" Height="900" Width="1600" WindowStartupLocation="CenterScreen" >
    <Grid Background="{StaticResource Chat}">
        <Grid.RowDefinitions>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0.08*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>

        <Grid Background="{StaticResource Channels}">
        </Grid>
        <Frame x:Name="frameMain" Grid.Column="1"/>
    </Grid>

</Window>
